.sign-com .modle .mol {
  width: 695rpx;
  height: 52rpx;
  overflow: hidden;
  position: relative;
  margin-top: 20rpx;
  margin-left: 10rpx;
}

.sign-com .mol-ites {
  width: 100%;
  height: 100%;
  position: absolute;
}

.moday .dd {
  width: 90rpx;
  font-size: 20rpx;
  color: #8d8d8d;
  position: absolute;
  left: -1rpx;
  bottom: 0;
}

.moday .dd:nth-of-type(2) {
  left: 110rpx;
}

.moday .dd:nth-of-type(3) {
  left: 220rpx;
}

.moday .dd:nth-of-type(4) {
  left: 330rpx;
}

.moday .dd:nth-of-type(5) {
  left: 440rpx;
}

.moday .dd:nth-of-type(6) {
  left: 550rpx;
  color: black;
}

.moday .dd:nth-of-type(7) {
  left: 660rpx;
  color: black;
}

.signin_text {
  width: 170rpx;
  font-size: 22rpx;
  color: red;
  margin: 0rpx auto;
  margin-top: 35rpx;
}

.mol-line {
  width: 695rpx;
  height: 7rpx;
  background-color: #e1e1e1;
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
}

.mol-ites {
  width: 100%;
  height: 100%;
  position: absolute;
}

.mol-ites .ite {
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  background: #e1e1e1;
  box-sizing: border-box;
  position: absolute;
  left: 0;
  top: 10rpx;
  z-index: 2;
}

.mol-ites .ite .n {
  text-align: center;
  border-radius: 50%;
  background-color: #e1e1e1;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  font-size: 22rpx;
}

.mol-ites .ite::after {
  content: "";
  width: 100rpx;
  height: 7rpx;
  background-color: transparent;
  position: absolute;
  left: 25rpx;
  top: 50%;
  margin-top: -2rpx;
  z-index: 2;
}

.mol-ites .ite:last-of-type::after {
  width: 0;
}

.mol-ites .ite:nth-of-type(2) {
  left: 100rpx;
}

.mol-ites .ite:nth-of-type(3) {
  left: 220rpx;
}

.mol-ites .ite:nth-of-type(4) {
  left: 330rpx;
}

.mol-ites .ite:nth-of-type(5) {
  left: 440rpx;
}

.mol-ites .ite:nth-of-type(6) {
  left: 550rpx;
}

.mol-ites .ite:nth-of-type(7) {
  left: 660rpx;
}

.mol-ites .ite.hover .n {
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  margin: 0rpx auto;
  background-color: #d288c3;
}

.mol-ites .ite.hover::after {
  width: 100rpx;
  background-color: #d288c3;
}

